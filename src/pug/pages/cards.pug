extends ../layout/main
block variables
  - var pageTitle = "UI Kit Cards"

block cards
  .wrapper
    include ../includes/modules/logo
    include ../includes/modules/mixin
    .cards_block
      .cards_block_section_first
        include ../includes/modules/cards/cards_find_number
        include ../includes/modules/cards/cards_registration_account  
      .cards_block_section_second
        include ../includes/modules/cards/cards_room_level 
        include ../includes/modules/cards/cards_entry


      .cards_block_section_third
        .cards_block-number_five-settings.bg-cards_settings-calendar.cards_block-left-space.cards_block-elements_space
        
        .cards_block-number_six-seven-settings
          .cards_block-number_six-settings.bg-cards_settings.cards_block-left-space.cards_block-elements_space
            #room_slider1.cards_block-settings_hotel-room_slider
              .cards_block-settings_hotel-room_arrows-gradient_wrapper
                .cards_block-settings_hotel-room_arrows-gradient
                  .cards_block-settings_hotel-room_left-arrow_gradient
                  .cards_block-settings_hotel-room_right-arrow_gradient  
              a(href='#').cards_block-settings_hotel-room_arrows.cards_block-settings_hotel-room_left-arrow 

              #room_picture1.cards_block-settings_hotel-room_picture
                img(src='assets/img/hotel_room1.jpg').hotel-room_picture.hotel-room_picture-active_img
                img(src='assets/img/hotel_room2.jpg').hotel-room_picture   
                img(src='assets/img/hotel_room3.jpg').hotel-room_picture
                img(src='assets/img/hotel_room4.jpg').hotel-room_picture       

              .cards_block-settings_hotel-room_slider-dots
